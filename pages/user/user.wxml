    <view class='my_sport_data card' catchtap="">
        <view class='my_sport_data_title'><text>我的运动数据</text> <image class='top_message_right' src='/icon/right.png'></image></view>
        <view class="flex">
          <view class="_data_number">
            <view>
              <text class='_data_number_title'>总运动</text><text class="small_font" >( 次数 )</text>
              <text class="alltime" selectable="false" space="false" decode="false">
                0
              </text>
            </view>
          </view>
          <view class="_data_number">
            <view>
              <text class='_data_number_title'>近一周运动</text><text class="small_font" >( 次数 )</text>
              <text class="alltime" selectable="false" space="false" decode="false">
                0
              </text>
            </view>
          </view>
        </view>
      </view>
      <view class='my_body_data card' catchtap="toUpdateBody">
        <view class='my_sport_data_title'><text>我的身体数据</text> <image class='top_message_right' src='/icon/right.png'></image></view>
        <view class='flex'>
          <view><text class="alltime">178.0</text><text class='small_font'>身高 cm</text></view>
          <view><text class="alltime">60</text><text class='small_font'>体重 kg</text></view>
        </view>
      </view>
      <view class='my_collect card_small flex' catchtap="">
        <view class='flex'>
          <image class='small_icon' src='/icon/sc.png'></image>
          <text class='tspx'>我的收藏</text>
        </view>
        <view><image class='top_message_right' src='/icon/right.png'></image></view>
      </view><!--
      <view class='my_purse card_small flex' catchtap="">
